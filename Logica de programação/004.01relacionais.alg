algoritmo "Nome do Programa"
var
    a: inteiro
    b: inteiro
    c: inteiro
inicio
    a <- 2
    b <- 3
    c <- 5
    escreval(a > b)
    escreval(a = b)
    escreval(a > 2)
    escreval(ab)
    escreval(a >= 2)
    escreval(c = a + b)
    escreval(c <= ba)
    escreval(c >= ba)
    escreval(b <= c % 2)
    escreval(1 = c % 2)

fimalgoritmo