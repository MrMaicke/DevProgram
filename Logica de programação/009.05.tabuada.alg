algoritmo "Tabuada"
var
    cont, num, res: inteiro
inicio
    cont <- 1
    escreva("Digite o numero que você quer a tabuada: ")
    leia(num)
    enquanto (cont <= 10) faca
        res <- cont * num
        escreval(num ," * ",cont ," : ", res)
        //numero vezes contagem igual multiplicação
        cont <- cont + 1
    fimenquanto
fimalgoritmo
