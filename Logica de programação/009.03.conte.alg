algoritmo "helpdivisão"
var
    contador, salto, valor: real
inicio
    contador <- 0
    escreva("Quer contar do 0 até qual valor? :")
    leia(valor)
    escreva("Qual o numero em vezes até o resultado? :")
    leia(salto)
    enquanto (contador <= valor) faca
        escreval(contador)
        contador <- contador + salto
    fimenquanto
fimalgoritmo
