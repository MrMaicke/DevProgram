algoritmo "primeira logica"
var
    a: inteiro
    b: inteiro
    c: inteiro
inicio
    a <- 2
    b <- 3
    c <- 5
    escreval(verdadeiro e falso)
    escreval((a + b) ou (c > a))
    escreval((a + b) ou (c > a))
    escreval(nao ((a + b) ou (c > a)))
    //nao inverte a logica ex: "nao VERDADEIRO" resulta em "FALSO, "nao FALSO" resulta em "VERDADEIRO.
fimalgoritmo