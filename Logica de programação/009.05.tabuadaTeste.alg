algoritmo "Tabuada teste"
var
    cont, num, res: inteiro
inicio
    cont <- 1
    escreva("Digite o numero que vocÃª quer a tabuada: ")
    leia(num)
    enquanto (cont <= 10) faca
        res <- cont * num
        escreval(num ," * ",cont ," : ", res)
        cont <- cont + 1
    fimenquanto
fimalgoritmo
